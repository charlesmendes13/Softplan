services:
  softplan.api1:
    build:
      context: D:\Visual Studio\Softplan\Softplan
      dockerfile: src/Api1/Softplan.Api1/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Softplan.Api1
      target: base
    container_name: Softplan.Api1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: softplanapi1:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8001
      target: 80
    tty: true
    volumes:
    - D:\Visual Studio\Softplan\Softplan\src\Api1\Softplan.Api1:/app:rw
    - C:\Users\Charles\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Charles\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Charles\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Charles\.nuget\packages:/root/.nuget/packages:ro
    - D:\Visual Studio\Softplan\Softplan:/src:rw
  softplan.api2:
    build:
      context: D:\Visual Studio\Softplan\Softplan
      dockerfile: src/Api2/Softplan.Api2/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Softplan.Api2
      target: base
    container_name: Softplan.Api2
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: softplanapi2:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8002
      target: 80
    tty: true
    volumes:
    - D:\Visual Studio\Softplan\Softplan\src\Api2\Softplan.Api2:/app:rw
    - C:\Users\Charles\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Charles\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Charles\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Charles\.nuget\packages:/root/.nuget/packages:ro
    - D:\Visual Studio\Softplan\Softplan:/src:rw
version: '3.4'