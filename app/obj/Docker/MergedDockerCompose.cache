services:
  softplan.api1:
    build:
      context: D:\Visual Studio\Softplan\app
      dockerfile: src/Api1/Softplan.Api1/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: softplanapi1
    ports:
    - published: 5001
      target: 80
  softplan.api2:
    build:
      context: D:\Visual Studio\Softplan\app
      dockerfile: src/Api2/Softplan.Api2/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: softplanapi2
    ports:
    - published: 5002
      target: 80
version: '3.4'